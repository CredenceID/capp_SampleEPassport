<resources>
    <declare-styleable name="FullscreenAttrs">
        <attr name="fullscreenBackgroundColor" format="color" />
        <attr name="fullscreenTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ScannerOverlayImpl">
        <attr name="type" format="enum">
            <enum name="IBAN" value="0"/>
            <enum name="MRZ" value="1"/>
            <enum name="QR" value="2"/>
        </attr>
    </declare-styleable>

</resources>